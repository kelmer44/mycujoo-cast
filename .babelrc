{
  "presets": [
    ["latest", {
      "es2015": {
        "modules": false
      }
    }],
    ["react"]
  ],

  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-es2015-spread",
    "transform-es2015-destructuring",
    ["transform-object-rest-spread", { "useBuiltIns": true }]
  ],
}
